PROGRAM CODE:ADD

MOV SI, 3000
MOV DI, 4002
MOV AX, [SI]
INC SI
INC SI
MOV [DI], 0000
ADD AX, [SI]
JNC 2014
MOV [DI], 0001
.DEC DI
DEC DI
MOV [DI], AX
HLT

INPUT:

3000: D5
3001: 7F
3002: 56
3003: EA

OUTPUT:

4000: 2B
4001: 6A
4002: 01

PROGRAM CODE:SUB

MOV SI, 3000
MOV DI, 4000
MOV AX, [SI]
INC SI
INC SI
MOV [DI], 0000
SUB AX, [SI]
JNC 2014
MOV [DI], 0001
.INC DI
MOV [DI], AX
HLT

INPUT:

3000: D5
3001: 7F
3002: 56
3003: EA

OUTPUT:

4000: 01
4001: 7F
4002: 95




PROGRAM CODE:MUL

MOV SI, 3000
MOV DI, 4000
MOV AX, [SI]
INC SI
INC SI
MOV BX, [SI]
MUL BX
MOV [DI], AX
INC DI
INC DI
MOV [DI], DX
HLT

INPUT:

3000: FF
3001: FF
3002: FF
3003: FF

OUTPUT:

4000: 01
4001: 00
4002: FE
4003: FF

PROGRAM CODE:DIV

MOV SI, 3000
MOV DI, 4000
MOV AX, [SI]
INC SI
INC SI
MOV BX, [SI]
MOV DX, 0000
DIV BX
MOV [DI], AX
INC DI
INC DI
MOV [DI], DX
HLT

INPUT:

